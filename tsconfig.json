{
  "compilerOptions": {
    "target": "ES5",
    "rootDir": "src",
    "outDir": "web/app",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": false,
    "jsx": "react-jsx",
  },
  "include": [
    "src",
  ],
}
